    <description>This is used to create handover task against the ERM records created via cerner queue trigger. Added check to skip using custom setting.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>today()</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_5447689485</name>
        <dataType>Date</dataType>
        <expression>today()</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>ISNEW()
&amp;&amp;
[Account].RecordType.DeveloperName == &apos;ERM_Patient&apos;
&amp;&amp;
[Account].Bypass_Cerner_Queue__c
&amp;&amp;
ispickval([Account].Case_Status__c, &apos;Active&apos; )
&amp;&amp;
ispickval([Account].Stage__c, &apos;Case - In CI Queue&apos;)
&amp;&amp;
[Account].Contact_or_Case_new__c == &apos;Case&apos;
&amp;&amp; 
(
	IF(
		ISNEW(),
		([Account].Process_EHR_Records__c == false),
		(
			OR (
				NOT( ISCHANGED( [Account].Test_Type_Generic__c) ),
				NOT( ISCHANGED( [Account].Test_Result_Generic__c) ),
				NOT( ISCHANGED( [Account].Test_Date_Generic__c) )
			) &amp;&amp;
			NOT($Setup.Skip_Account_Automation__c.Skip_Account_Process_Builder__c)
		)
	)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>ISNEW()
&amp;&amp;
{!myVariable_current.RecordType.DeveloperName} == &apos;ERM_Patient&apos;
&amp;&amp;
{!myVariable_current.Bypass_Cerner_Queue__c}
&amp;&amp;
ispickval({!myVariable_current.Case_Status__c}, &apos;Active&apos; )
&amp;&amp;
ispickval({!myVariable_current.Stage__c}, &apos;Case - In CI Queue&apos;)
&amp;&amp;
{!myVariable_current.Contact_or_Case_new__c} == &apos;Case&apos;
&amp;&amp; 
(
	IF(
		ISNEW(),
		({!myVariable_current.Process_EHR_Records__c} == false),
		(
			OR (
				NOT( ISCHANGED( {!myVariable_current.Test_Type_Generic__c}) ),
				NOT( ISCHANGED( {!myVariable_current.Test_Result_Generic__c}) ),
				NOT( ISCHANGED( {!myVariable_current.Test_Date_Generic__c}) )
			) &amp;&amp;
			NOT({!$Setup.Skip_Account_Automation__c.Skip_Account_Process_Builder__c})
		)
	)
)</expression>
    </formulas>
