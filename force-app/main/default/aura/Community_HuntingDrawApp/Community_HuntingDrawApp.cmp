                                                    required="true" 
                                                    options="{!v.haveLicenseOptions}"
                                                    value="{!v.value}"
                                                    type="radio"
                                                    onblur="{!c.clearError}"/>
                      </div>
                    </div>
                  </div>
                <!-- /License Holder-->

                 <!--Parent/Guardian Information-->
