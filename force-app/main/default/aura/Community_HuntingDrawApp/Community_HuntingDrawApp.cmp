                    </div> 
                    
                    
                <!-- </div>

                <div class="row ">  -->
                    <div class=" col-sm-4"> 
                        <!-- <lightning:input label="Mobile"
                                             aura:id="aIdPhone"
                                             class="alignField"
                                             required="true"
                                             onblur="{!c.clearErrors}"
                                             value="{! v.accObj.PersonMobilePhone}"
                                             pattern="/^\(?([0-9]{3})\)?[-. ]?([0-9]{3})[-. ]?([0-9]{4})$/"
                                             />    -->
                        <span id="aIdPhone" onkeypress="{!c.NumberCheck}" onkeyup="{!c.NumberCheck}">
                            <lightning:input label="Mobile"
                                             aura:id="aIdPhone"
                                             class="alignField"
                                             required="true"
                                             onblur="{!c.clearErrors}"
                                             value="{! v.accObj.PersonMobilePhone}"
                                             maxlength="13"
                                             oncontextmenu="{!c.handleContext}" 
                       					     onpaste="{!c.handlePaste}"
                                             ondrop="{!c.handleContext}"
                                             />   
                            </span>
                      
                   </div>
                </div>
                <!--/Applicant Information-->

                <!--Physical Address-->
                <!-- <div class="panel-head2" >  <h5 class="mainHeading text-center">Physical Address</h5> </div> -->
                  <h1 class="head">Physical Address:</h1> 
                  <div class="row">
                      <div class="col-sm-12" > 
                        <strong>
                        <lightning:input type="checkbox"  
                                         required="true"
                                         style = 'margin-top: 22px;'
                                         label="I have verified that my address is correct"
                                         class="alignField"
                                         aura:id="acceptCheckBox" 
                                         checked="{!v.iAccept}" />
                        </strong>
                     </div>

                      <div class="col-sm-6 " >  
                          <lightning:input label="Street" 
                                           aura:id="PhysicalStreetInput" 
                                           class="alignField"
                                           required="{!v.physicalAddressRequired}"
                                           value="{! v.accObj.PersonOtherStreet}"
                                           onchange="{! c.handlePhysicalStreet}"
                                           disabled="{! not(v.p_isManual)}"/>
                          
                      </div>
                      <div class="col-sm-6 " >  
                          <lightning:input label="Address Line2" 
                                           aura:id="PhysicalAddress2Input" 
                                           class="alignField" 
                                           value=""
                                           onchange="{! c.handlePhysicalAddress2}"
                                           disabled="{! not(v.p_isManual)}"/>
                          
                      </div>
                      <div class="col-sm-6 " >  
                          <lightning:input label="Suite" 
                                           aura:id="PhysicalSuiteInput" 
                                           class="alignField" 
                                           value="{! v.accObj.Physical_Suite__c}"
                                           onchange="{! c.handlePhysicalSuite}"
                                           disabled="{! not(v.p_isManual)}"/>
                          
                      </div>
                      <div class="col-sm-6 " >  
                          <lightning:input label="City" 
                                           aura:id="PhysicalCityInput" 
                                           class="alignField "
                                           value="{! v.accObj.PersonOtherCity}"
                                           disabled="{! not(v.p_isManual)}"
                                           onchange="{! c.handlePhysicalCity}"
                                           required="{!v.physicalAddressRequired}"/>
                      </div>
                      
                      <div class="col-sm-6" >
                           <lightning:select class="alignField inputField"
                                             name="PhysicalCountryInput"
                                             aura:id="PhysicalCountryInput"
                                             value=""
                                             label="Country"
                                             disabled="{! not(v.p_isManual)}"
                                             onchange="{!c.handleCountryChange}"
                                             required="{!v.physicalAddressRequired}">
                                 <option value="">--None--</option>
                                <aura:iteration items="{!v.getParentList}" var="value">
                                    <option value="{!value}">{!value}</option>
                                </aura:iteration>
                            </lightning:select>   

                          <!-- <lightning:input  label="Country"
                                            aura:id="PG_PhysicalCountryInput"
                                            class="alignField "
                                            disabled="{! not(v.appl_PhysAdd_isManual)}"/> -->
                      </div>  
                      
                      <aura:if isTrue="{!v.showPhysicalState}">
                      <div class="col-sm-6 " >  
                          <lightning:select class="alignField inputField"
                                            aura:id="PhysicalStateInput"
                                            name="childField"
                                            value=""
                                            label="State"
                                            onchange="{! c.handlePhysicalStateChange}"
                                            disabled="{! not(v.p_isManual)}"
                                            required="{!v.physicalStateRequired}">
                                     <option value="">--None--</option>
                                    <aura:iteration items="{!v.getChildList}" var="value">
                                        <option value="{!value}">{!value}</option>
                                    </aura:iteration>
                                </lightning:select> 
                             
                      </div>
                    </aura:if>

                    <aura:if isTrue="{!v.showPhysicalOtherState}">
                        <div class="col-sm-6 ">
                            <lightning:input type="text"
                                             aura:id="PhysicalOtherState"
                                             name="PhysicalOtherState"
                                             label="Other State/Territory: " 
                                             class="alignField inputField"
                                             disabled="{!not(v.p_isManual)}"
                                             onchange="{! c.handlePhysicalOtherState}"
                                             required="{!v.physicalOtherStateRequired}"/>                			                           
                        </div>
                    </aura:if>

                      <!-- <div class="col-sm-6" >               
                        <lightning:input label="County"  
                                         aura:id="physicalCounty" 
                                         class="alignField "
                                         value=""
                                         disabled="{! not(v.p_isManual)}"/>
                    </div> -->
                      
                       <div class="col-sm-6" >               
                          <lightning:input label="Zip Code"  
                                           aura:id="PhysicalzipInput" 
                                           class="alignField "
                                           value="{! v.accObj.PersonOtherPostalCode}"
                                           disabled="{! not(v.p_isManual)}"
                                           onchange="{! c.handlePhysicalZip}"
                                           required="{!v.physicalAddressRequired}"/>  
                      </div>

                      <div class="col-sm-6" > 
                          
                        <Strong>
                          <label class="slds-form-element__label" style = 'margin-top: 8px;' for="toggle">Update Address</label>
                          </Strong>  
                          <lightning:input class="cell"
                                           type="toggle" 
                                           aura:id="addressUpdateCheckBox" 
                                           checked="{!v.addressUpdate}"
                                           onchange="{!c.handleAddreesUpdate}"
                                           messageToggleActive="Yes"
                                           messageToggleInactive="No"
                                           required="{!v.UpdateAddressRequired}"/>
                      </div>

                    <aura:if isTrue="{!v.showLookUp}">
                      <div class="col-sm-6">
                        <lightning:input aura:id="p_AddressFreeFormInput" 
                                         class="alignField" 
                                         label="Address Lookup: "  
                                         value="{!v.p_location}"
                                         onchange="{!c.handlePhysicalAddressChange}"
                                         required="{!v.p_FFAddressRequired}"/>
                        <aura:if isTrue="{!v.p_predictions.length > 0}">		 
                        <ul class="result" id="p_addressResult">
                            <aura:iteration items="{!v.p_predictions}" var="prediction" indexVar="indexVar">
                                    <aura:if isTrue="{!prediction.Address.SuiteCount > 0}">
                                    <aura:iteration items="{!prediction.Address.SuiteList}" var="suite" indexVar="indexVar2">
                                        <li class="slds-listbox__item">
                                            <a onclick="{!c.handlePhysicalAddressClick}" data-placeid="{!indexVar+indexVar2}" data-place="{!prediction.Address.AddressLine1 +' '+suite+', '+ prediction.Address.City+', '+prediction.Address.State}">{!prediction.Address.AddressLine1 +' '+suite+', '+ prediction.Address.City+', '+prediction.Address.State}</a>
                                        </li>
                                    </aura:iteration>
                                    <aura:set attribute="else">
                                        <li class="slds-listbox__item">
                                            <a onclick="{!c.handlePhysicalAddressClick}" data-placeid="{!indexVar}" data-place="{!prediction.Address.AddressLine1 + (prediction.Address.SuiteName != ''?' '+prediction.Address.SuiteName:'') +', '+ prediction.Address.City+', '+prediction.Address.State}">
                                            {!prediction.Address.AddressLine1} <aura:if isTrue="{!prediction.Address.SuiteName != ''}">{!' '+prediction.Address.SuiteName}</aura:if> {!', '+ prediction.Address.City+', '+prediction.Address.State}</a>
                                        </li>
                                    </aura:set>
                                </aura:if> 
                            </aura:iteration>
                        </ul> 	 
                     </aura:if>
                     </div>

                     <div class="col-sm-6">
