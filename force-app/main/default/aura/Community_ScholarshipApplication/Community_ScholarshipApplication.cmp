<aura:component  implements="force:lightningQuickAction,forceCommunity:availableForAllPageTypes" access="global" controller="Community_ScholarshipApplication">
    <ltng:require styles="{!$Resource.CN_Boot}"/>
    <!--Allow Multi Line Space in Toast Messages-->
    <aura:html tag="style">
        .toastMessage.forceActionsText{
        white-space : pre-line !important;
        } 
    </aura:html>
     <aura:attribute name="addressClicked" type="boolean" default="false"/>
    <aura:attribute name="CVCase" type="Case"
                    default="{ 'sobjectType': 'Case'
                             }"/>
    <aura:attribute name="CVCaseClone" type="Case"
                    default="{ 'sobjectType': 'Case'
                             }"/>
    <aura:attribute name="Acnt" type="Account"/>
    
    <aura:attribute name="showSuccessMessage" type="boolean" default="false"/>
    <aura:attribute name="showWarningMessage" type="boolean" default="false"/>
    <aura:attribute name="showPrimaryGuardianSection" type="boolean" default="false"/>
    <aura:attribute name="showHousingStipend" type="boolean" default="true"/>
    <aura:attribute name="showReviewScreen" type="boolean" default="false"/>
    <aura:attribute name="UpdateAddressRequired" type="boolean" default="false"/>
    <aura:attribute name="UpdatePhysicalAddressRequired" type="boolean" default="false"/>
    
    <!--Attributes for Mailing Melissa Data-->
    <aura:attribute name="location" type="string" default=""/>
    <aura:attribute name="predictions" type="List" default="[]"/>
    <aura:attribute name="states" type="List" default="[]"/>
    <aura:attribute name="isManual" type="boolean" default="false"/>
    <aura:attribute name="selectedAddressIndex" type="Integer" default="0"/>
    <aura:attribute name="isAddressChanged" type="boolean" default="false"/>
    <aura:attribute name="selectedAddress" type="string" default=""/>
    <aura:attribute name="showManualCB" type="boolean" default="false"/>
    <aura:attribute name="showLookUp" type="boolean" default="false"/>
    <aura:attribute name="applicationResult" type="string"/>
    
    <!--Attributes for Physical Melissa Data-->
    <aura:attribute name="locationPA" type="string" default=""/>
    <aura:attribute name="predictionsPA" type="List" default="[]"/>
    <aura:attribute name="isPAManual" type="boolean" default="false"/>
    <aura:attribute name="selectedPhyAddressIndex" type="Integer" default="0"/>
    <aura:attribute name="isPhyAddressChanged" type="boolean" default="false"/>
    <aura:attribute name="selectedPhyAddress" type="string" default=""/>
    <aura:attribute name="showPAManualCB" type="boolean" default="false"/>
    <aura:attribute name="showPALookUp" type="boolean" default="false"/>
    
    <aura:attribute name="caseId" type="String"/>
    <aura:attribute name="requestNumber" type="String"/>
    
    <!--Attributes for Students-->
    <aura:attribute name="StudentsList" type="CA_Student2__c[]" /> 
    <aura:attribute name="ProcessedStudentsList" type="CA_Student2__c[]" /> 
    
    <!--Attributes for HH Members-->
    <aura:attribute name="HHMembersList" type="HH_Member__c[]" /> 
    
    <!--Attributes for Required-->
    <aura:attribute name="mailingAddressRequired" type="boolean" default="false"/>
    <aura:attribute name="FFAddressRequired" type="boolean" default="false"/>
    <aura:attribute name="physicalAddressRequired" type="boolean" default="false"/>
    <aura:attribute name="FFPhysicalAddressRequired" type="boolean" default="false"/>
    <aura:attribute name="HHMembersRequired" type="boolean" default="false"/>
    
    <!--Attributes for Physical and Mailing State/Country Picklists-->
    <aura:attribute name="getMailingParentList" type="list" default="[]" description=""/>
    <aura:attribute name="getMailingChildList" type="list" default="[]"/>
    <aura:attribute name="getPhysicalParentList" type="list" default="[]" description=""/>
    <aura:attribute name="getPhysicalChildList" type="list" default="[]"/>
    <aura:attribute name="getPickListMap" type="map"/>
    
    <!--Attributes to show hide State/Other State Fields-->
    <aura:attribute name="showMailingState" type="boolean" default="true"/>
    <aura:attribute name="showMailingOtherState" type="boolean" default="false"/>
    <aura:attribute name="showPhysicalState" type="boolean" default="true"/>
    <aura:attribute name="showPhysicalOtherState" type="boolean" default="false"/>
    
    <!--Attributes for Button -->
    <aura:attribute name="iAccept" type="Boolean" default="false"/>
    <aura:attribute name="statementUnderstanding" type="Boolean" default="false"/>
    <aura:attribute name="malingAddressCorrect" type="Boolean" default="false"/>
    <aura:attribute name="addressUpdate" type="Boolean" default="false"/>
    <aura:attribute name="physicalAddressCorrect" type="Boolean" default="false"/>
    <aura:attribute name="physicalAddressUpdate" type="Boolean" default="false"/>
    <aura:attribute name="isActive2" type="Boolean" default="false"/>
    <aura:attribute name="isActive" type="Boolean" default="false"/>
    <aura:attribute name="univ" type="string" default=""/>
    <aura:attribute name="univName" type="string" default=""/>
    
    
    <!--Attributes for Picklists-->
    <aura:attribute name="genderList" type="String[]" />
    <aura:attribute name="maritalList" type="String[]" />
    <aura:attribute name="tribeList" type="String[]" />
    <aura:attribute name="districtList" type="String[]" />
    <aura:attribute name="semesterList" type="String[]" />
    <aura:attribute name="classificationList" type="String[]" />
    <aura:attribute name="monthList" type="String[]" />
    <aura:attribute name="fieldOfStudyList" type="String[]" />
    <aura:attribute name="degreeList" type="String[]" />
    <aura:attribute name="studentTypeList" type="String[]" />
    <aura:attribute name="applicationTypeList" type="String[]" />
        <aura:attribute name="relationList" type="String[]" />
     <aura:attribute name="countyList" type="String[]" />

    
    
    
    <aura:attribute name="filetype" type="List" default="['.png', '.jpg', '.jpeg','.pdf']"/>
    <aura:attribute name="incomeUploadedFilesList" type="List" default="[]" access="public"/>
    <aura:attribute name="uploadedTranscriptList" type="List" default="[]" access="public"/>
    <aura:attribute name="uploadedLetterList" type="List" default="[]" access="public"/>
    <aura:attribute name="uploadedw9List" type="List" default="[]" access="public"/>
    <aura:attribute name="uploadedOtherList" type="List" default="[]" access="public"/>
    
    
    
    
    <aura:attribute name="isLoading" type="boolean" default="false"/>
    
    
    <aura:attribute name="applicationTypes" type="String[]"/>
    
    <!--
    {'label': 'Concurrent Enrollment Scholarship', 'value': 'concurrentEnrollment'},
    {'label': 'Cherokee Nation Scholarship', 'value': 'cherokeeNation'},
    {'label': 'Valedictorian and Salutatorian', 'value': 'valSal'}, -->
    <aura:attribute name="applicationValue" type="String" default=""/>
    
    
    <aura:attribute name="studentTypeValue" type="String" default=""/>
    
    
    <aura:attribute name="degreeValue" type="String" default=""/>
    
    
    <aura:attribute name="semesterValue" type="String" default=""/>
    <!--Register Events-->
    <aura:registerEvent name="vendorLookup" type="c:customLookupEvt"/>
    <aura:attribute name="searchTerm" type="String" default=""/>
    <aura:attribute name="rows" type="Map[]"/>
    
    
    
    
    <!--Handler-->
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/> 
    
    <aura:if isTrue="{!not(v.showSuccessMessage)}">
        <div class="main-container"> 
            <aura:if isTrue="{!v.isLoading}">
                <lightning:spinner aura:id="mySpinner" variant="brand" />
                <aura:set attribute="else">
        <div class="card m-box ">
                <img src="{!$Resource.Scholarship_AppHeader}" alt=""/>
                <div class="row">        
                    <div class="col-sm-12" >  
                        <div class="panel-head" style="background-color: #43476A;">  
                            <h5 class="mainHeading text-center" style="color:#fff">
                                Online Scholarship Application
                            </h5> </div>
                        <div class="m-Border "></div>
                    </div>
                </div>
                
                <div class="m-box-style">
                    <br/>
                    <aura:if isTrue="{!v.showWarningMessage}">
                      <div class="panel-head2">  <h5 class="mainHeading text-center"><aura:unescapedHtml value="{!v.applicationResult}"/><br></br>Please only proceed if you need to submit a second application.</h5> </div>

                    </aura:if>
                    
                    <p style="margin-top: 0px;">Osiyo! Welcome to the Cherokee Nation Scholarship Program!</p>
                    <br/>
                    <p style="margin-top: 0px;">Please complete the online application to apply for scholarship funding for the upcoming academic year by uploading required documents saved in a PDF format. Applications will be processed in order of receipt. Applications received after posted deadlines will not be processed.</p>
                    <br/>
                    <p style="margin-top: 0px;">Note: You will need to complete this application process in a single setting. If you stop before completing the process, you will need to begin again.</p>
                    <br/>
                    <p style="margin-top: 0px;"><a href="{!$Label.c.Community_ScholarshipDoc}" target="_blank">Click here</a> to see a list of required documents that must be saved individually in a PDF format to attach to this application.</p>
                    <br/>
                    <p style="margin-top: 0px;"><a href="{!$Label.c.Community_ScholarshpDistricts }" target="_blank">Click here</a> to see Higher Education Service Area Map.</p>
                    <br/>
                    <p style="margin-top: 0px;"><b>Residency Restrictions:</b></p>
                    
                    <p style="margin-top: 0px;">Undergraduate (Not eligible for Federal Pell), Graduate and Valedictorian/Salutatorian: must reside within the Cherokee Nation Reservation Area or contiguous counties (inside the highlighted counties).</p>
                    <br/>
                    <p style="margin-top: 0px;">Undergraduate (Pell eligible): must reside within the United States.</p>
                    <br/>
                    <p style="margin-top: 0px;">Concurrent Enrollment: must reside within the Cherokee Nation Reservation Area or contiguous counties (inside the highlighted counties).</p>
                    <br/>
                    <p style="margin-top: 0px;">The scholarships are intended to supplement educational needs for those pursuing a degree from an accredited post-secondary insitution and shall be contingent on availability of funding.</p>
                    <br/>
                    <p style="margin-top: 0px;"><b>Please allow 8 weeks for processing.</b></p>
                    <br/>
                    <p style="margin-top: 0px;">Wado,</p>
                    <p style="margin-top: 0px;">Cherokee Nation College Resource Center</p>
                    <br/>
                    
                    <p style="margin-top: 0px;"><b>Application Enrollment Periods:</b></p>
                    <p>
                        <ul class="entry-content" style="list-style-type: square;padding-left:20px;">
                            <li>High School Concurrent Enrollment</li>
                            <ul class="entry-content" style="list-style-type: none;padding-left:20px;">
                                <li>March 1, 2022 - June 15, 2022 11:59 PM CT.</li>
                            </ul>
                            <li>Cherokee Nation Scholarship</li>
                            <ul class="entry-content" style="list-style-type: none;padding-left:20px;">
                                <li>March 1, 2022 - June 15, 2022 11:59 PM CT.</li>
                            </ul>
                            <li>Valedictorian and Salutatorian (Class of 2022)</li>
                            <ul class="entry-content" style="list-style-type: none;padding-left:20px;">
                                <li>March 1, 2022 - March 31, 2022 11:59 PM CT.</li>
                            </ul>
                        </ul>
                    </p>
                    <br/>
                    
                    <!--Application Type-->
                    <div class="panel-head2" >  <h5 class="mainHeading text-center">Application Type</h5> </div>
                    <h1 class="head">Which Scholarship are you applying for?</h1>  
                    <div class="row ">
                        <div class=" col-sm-8"> 
                            <lightning:radioGroup name="applicationRequired"
                                                  required="true"
                                                  options="{! v.applicationTypeList }"
                                                  value="{! v.CVCase.Scholarship_Application__c }"
                                                  type="radio"
                                                  aura:id="applicationType"
                                                  onchange="{!c.radioChange}"
                                                  
                                                  
                                                  />
                        </div>
                    </div>
                    <aura:if isTrue="{!and(v.CVCase.Scholarship_Application__c!='Valedictorian and Salutatorian',v.CVCase.Scholarship_Application__c!='')}">
                        <h1 class="head">Are you a new or continuing scholarship applicant?</h1>  
                        <div class="row ">
                            <div class=" col-sm-4"> 
                                <lightning:radioGroup name="studentStatus"
                                                      aura:id="studentStatus"
                                                      required="true"
                                                      options="{! v.studentTypeList }"
                                                      value="{! v.CVCase.Student_Type__c }"
                                                      type="radio"
                                                       onchange="{!c.radioChange}"
                                                      
                                                      />
                            </div>
                        </div>
                        <aura:if isTrue="{!(v.CVCase.Scholarship_Application__c!='Concurrent Enrollment Scholarship')}">
                            <h1 class="head">Are you an undergraduate or graduate student?</h1>  
                            <div class="row ">
                                <div class=" col-sm-4"> 
                                    <lightning:radioGroup name="degreeSeek"
                                                          aura:id="degreeSeek"
                                                          required="true"
                                                          options="{! v.degreeList }"
                                                          value="{! v.CVCase.Degree__c  }"
                                                          type="radio"
                                                           onchange="{!c.radioChange}"
                                                          />
                                </div>
                            </div>
                        </aura:if>
                        
                    </aura:if>
                    
                    <!--/Application Type-->    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    <!--Personal Information-->
                    <div class="panel-head2" >  <h5 class="mainHeading text-center">Personal Information</h5> </div>
                    <h1 class="head">Legal Name</h1>  
                    <div class="row ">
                        <div class=" col-sm-4"> 
                            <lightning:input label="First Name" 
                                             aura:id="priConFirstName"
                                             value="{!v.CVCase.Primary_Contact_First_Name__c}"
                                             class="alignField" 
                                             type="text"
                                             disabled="true"/>       
                        </div>
                        
                        <div class=" col-sm-4"> 
                            <lightning:input label="Middle Name"
                                             aura:id="priConMiddleName"
                                             value="{!v.Acnt.Middle_Name__c}"
                                             class="alignField"
                                             type="text"
                                             disabled="true"/>       
                        </div>
                        
                        <div class=" col-sm-4"> 
                            <lightning:input label="Last Name"
                                             aura:id="priConLastName"
                                             value="{!v.CVCase.Primary_Contact_Last_Name__c}"
                                             class="alignField "
                                             type="text"
                                             disabled="true"/>       
                        </div>
                        <div class=" col-sm-4"> 
                            <lightning:input label="Maiden Name"
                                             aura:id="priConMaidenName"
                                             value="{!v.CVCase.Primary_Contact_Maiden_Name__c}"
                                             class="alignField "
                                             type="text"
                                             onchange="{!c.validateLName}"
                                             disabled="false"/>       
                        </div>
                    </div>
                    <div class="row">
                        
                        <div class=" col-sm-4">
                            <lightning:input label="Cherokee Nation Citizen Registry ID #"
                                             aura:id="RegID"
                                             value="{!v.Acnt.Tribe_Id__c}"
                                             class="alignField "
                                             type="text"
                                             disabled="true"/>
                        </div>
                        <div class=" col-sm-4">
                            <lightning:input label="Social Security #"
                                             aura:id="ssn"
                                             value="{!v.Acnt.Social_Security__c}"
                                             class="alignField "
                                             type="text"
                                             required="true"
                                             onchange = "{!c.formatSSN}"
                                             disabled="false"/>
                        </div>
                        <div class=" col-sm-4">
                            <lightning:input label="College Student ID #"
                                             aura:id="studentId"
                                             value="{!v.CVCase.Student_ID__c}"
                                             class="alignField "
                                             type="text"
                                             required="true"
                                    
                                             disabled="false"/>
                        </div>
                        <div class=" col-sm-4"> 
                            <lightning:input class="alignField"
                                             label="Date of Birth"
                                             aura:id="priConDob"
                                             value="{!v.Acnt.Date_of_Birth__c}"
                                             disabled="true"
                                             placeholder="MM/DD/YYYY"/>               
                        </div> 
                        <div class=" col-sm-4"> 
                            <lightning:input aura:id="priConEmail" 
                                             label="Email" 
                                             value="{!v.Acnt.Users[0].Email}"
                                             type="email"
                                             class=" alignField" 
                                             disabled="true"/>      
                        </div>
                        <div class=" col-sm-4"> 
                            <lightning:input aura:id="secondaryEmail" 
                                             label="Secondary Email" 
                                             value="{!v.CVCase.Sec_Email__c}"
                                             type="email"
                                             class=" alignField"
                                            onchange="{!c.validateEmail}"
                                             disabled="false"/>      
                        </div>
                        
                        <div class=" col-sm-4"> 
                            <lightning:input label="Mobile"
                                             aura:id="priConCellPhone"
                                             value="{!v.CVCase.Ebt_Mobile_Phone__c}"
                                             class="alignField"
                                             required="true"
                                             onblur="{!c.clearErrors}"
                                             maxlength="14"  
                                             onchange="{!c.formatPhone}"
                                             />
                        </div>
                        <div class=" col-sm-4"> 
                            <lightning:select class="alignField" aura:id="gender" name="gender" label="Gender" required="false" value="{!v.CVCase.Gender__c}" onchange="{!c.clearErrors}">
                                <option value="">--None--</option>
                                
                            <aura:iteration items="{!v.genderList}" var="gender" indexVar="key" >
                                    <option text="{!gender.value}" value="{!gender.key}" selected="{!gender.value == v.CVCase.Gender__c}"></option>
                                </aura:iteration>
                                </lightning:select>
                            
                        </div>
                        <div class=" col-sm-4"> 
                            <lightning:select class="alignField" aura:id="maritalStatus" name="maritalStatus" label="Marital Status" required="false" value="{!v.CVCase.Marital_Status__c}" onchange="{!c.clearErrors}">
                                <option value="">--None--</option>
                                <aura:iteration items="{!v.maritalList}" var="marital" indexVar="key" >
                                    <option text="{!marital.value}" value="{!marital.key}" selected="{!marital.value == v.CVCase.Marital_Status__c}"></option>
                                </aura:iteration>
                            </lightning:select>
                            
                        </div>
                        <div class=" col-sm-4"> 
                            <lightning:select class="alignField" aura:id="tribe" name="tribe" label="Tribe" required="false" value="{!v.CVCase.Tribe__c}" onchange="{!c.clearErrors}">
                                <option value="">--None--</option>
                                <aura:iteration items="{!v.tribeList}" var="tribe" indexVar="key" >
                                    <option text="{!tribe.value}" value="{!tribe.key}" selected="{!tribe.value == v.CVCase.Tribe__c}"></option>
                                </aura:iteration>
                            </lightning:select>
                            
                        </div>
                        
                        
                        
                        
                        
                        
                    </div>
                    <div class="row">
                        <div class="col-sm-12">
                            <lightning:textarea name="input1" 
                                                label="Career Goals" 
                                                maxlength="1000"
                                                value="{!v.CVCase.Description}"
                                                placeholder="Please describe your current Career/Educational goals in 1000 characters or less."/>
                            
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-4">
                            <lightning:input type="checkbox"
                                             required="false"
                                             style = 'margin-top: 22px;'
                                             label="First generation college student?"
                                             class="alignField"
                                             aura:id="firstGen"
                                             checked="{!v.CVCase.First_Generation_College_Student__c }" />
                        </div>
                        <div class="col-sm-4">
                            <lightning:input type="checkbox"
                                             required="false"
                                             style = 'margin-top: 22px;'
                                             label="Do you speak, read, or write Cherokee?"
                                             class="alignField"
                                             aura:id="cherokeeFluency"
                                             checked="{!v.CVCase.Do_you_speak_read_or_write_Cherokee__c }" />
                        </div>
                    </div>
                    
                    <!--/Personal Information-->
                    
                    
                    <!--Mailing Address-->  
                    <div class="panel-head2" >  <h5 class="mainHeading text-center">Mailing Address</h5> </div>
                    <h1 class="head" style="margin-bottom: -1%;">Mailing Address (If your mailing address is not correct, please click the update address toggle and use the Address Lookup field that appears):</h1>  
                    <div class="row">     
                        <div class="col-sm-12 " >  
                            <strong>
                                <lightning:input type="checkbox"  required="true"
                                                 style = 'margin-top: 22px;'
                                                 label="I have verified that my mailing address is correct"
                                                 class="alignField"
                                                 aura:id="mAddressCheckBox" checked="{!v.malingAddressCorrect}" />
                            </strong>
                        </div>
                        <div class="col-sm-6 " >  
                            <lightning:input aura:id="MailingStreetInput" 
                                             class="alignField" 
                                             value="{!v.CVCase.Mailing_Street__c}"
                                             label="Address Line1: " 
                                             disabled="{!not(v.isManual)}"
                                             required="{!v.mailingAddressRequired}" 
                                             onblur="{!c.clearErrors}"/>
                            
                        </div>
                        <div class="col-sm-6 " >  
                            <lightning:input aura:id="MailingAddress2Input" 
                                             class="alignField" 
                                             value="{!v.CVCase.mailing_address2__c}"
                                             label="Address Line2: " 
                                             disabled="{!not(v.isManual)}"
                                             onblur="{!c.clearErrors}"/>
                            
                        </div>
                        <div class="col-sm-6 " >  
                            <lightning:input aura:id="MailingSuiteInput" 
                                             class="alignField" 
                                             value="{!v.CVCase.mailing_suite__c}"
                                             label="Suite: " 
                                             disabled="{!not(v.isManual)}"
                                             onblur="{!c.clearErrors}"/>
                            
                        </div>
                        <div class="col-sm-6 " >  
                            <lightning:input aura:id="MailingCityInput" 
                                             class="alignField "
                                             value="{!v.CVCase.Mailing_City__c}"
                                             label="City: " 
                                             disabled="{!not(v.isManual)}"
                                             required="{!v.mailingAddressRequired}"
                                             onblur="{!c.clearErrors}"/>
                        </div>
                        
                        <div class="col-sm-6" >
                            <lightning:select class="alignField "
                                              name="MailingCountryInput"
                                              value="{!v.CVCase.Mailing_Countries__c}"
                                              aura:id="MailingCountryInput"
                                              label="Country"
                                              disabled="{!not(v.isManual)}"
                                              required="{!v.mailingAddressRequired}"
                                              onblur="{!c.clearErrors}"
                                              onchange="{!c.onMailingCountryChange}">
                                
                                <option value="">--None--</option>
                                <aura:iteration items="{!v.getMailingParentList}" var="value">
                                    <option value="{!value}" selected="{!value==v.CVCase.Mailing_Countries__c}">{!value}</option>
                                </aura:iteration>
                                
                            </lightning:select>    
                        </div>  
                        
                        <aura:if isTrue="{!v.showMailingState}">
                            <div class="col-sm-6" > 
                                <lightning:select class="alignField "
                                                  aura:id="MailingStateInput"
                                                  value="{!v.CVCase.Mailing_States__c}"
                                                  name="childField"
                                                  label="State"
                                                  disabled="{!not(v.isManual)}"
                                                  required="{!v.mailingAddressRequired}"
                                                  onblur="{!c.clearErrors}">
                                    
                                    <option value="">--None--</option>
                                    <aura:iteration items="{!v.getMailingChildList}" var="value">
                                        <option value="{!value}" selected="{!value==v.CVCase.Mailing_States__c}">{!value}</option>
                                        
                                    </aura:iteration>
                                    
                                </lightning:select>    
                            </div>
                        </aura:if>
                        
                        <aura:if isTrue="{!v.showMailingOtherState}">
                            <div class="col-sm-6 " >  
                                <lightning:input aura:id="MailingOtherState" 
                                                 class="alignField inputField"
                                                 value="{!v.CVCase.Other_Mailing_State__c}"
                                                 label="Other State/Territory:" 
                                                 disabled="{!not(v.isManual)}"
                                                 required="{!v.mailingAddressRequired}"
                                                 onblur="{!c.clearErrors}"/>
                            </div>
                        </aura:if>
                        
                        <div class="col-sm-6" >               
                            <lightning:input aura:id="MailingzipInput" 
                                             class="alignField "
                                             value="{!v.CVCase.Mailing_ZipPostal_Code__c}"
                                             label="Zip Code: "  
                                             disabled="{!not(v.isManual)}"
                                             required="{!v.mailingAddressRequired}"
                                             onblur="{!c.clearErrors}"/>
                        </div>
                        
                        <div class="col-sm-6" > 
                            
                            <Strong>
                                <label class="slds-form-element__label" style = 'margin-top: 8px;' for="toggle">Update Mailing Address</label>
                            </Strong>  
                            <lightning:input class="cell"
                                             type="toggle" 
                                             aura:id="addressUpdateCheckBox" 
                                             checked="{!v.addressUpdate}"
                                             onchange="{!c.handleAddreesUpdate}"
                                             messageToggleActive="Yes"
                                             messageToggleInactive="No"
                                             required="{!v.UpdateAddressRequired}"/>
                        </div>
                        
                        <aura:if isTrue="{!v.showLookUp}">
                            <div class="col-sm-6">
                                <lightning:input aura:id="AddressFreeFormInput" 
                                                 class="alignField" 
                                                 label="Address Lookup: "  
                                                 value="{!v.location}"
                                                 onchange="{!c.handleAddressChange}"
                                                 required="{!v.FFAddressRequired}"/>
                                <aura:if isTrue="{!v.predictions.length > 0}">		 
                                    <ul class="result" id="addressResult">
                                        <aura:iteration items="{!v.predictions}" var="prediction" indexVar="indexVar">
                                            <aura:if isTrue="{!prediction.Address.SuiteCount > 0}">
