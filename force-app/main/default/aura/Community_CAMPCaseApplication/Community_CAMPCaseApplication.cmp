                                <lightning:textarea name="CollegeCareerGoalStatement"  class="alignField modal__FontSize col_14" value="{!v.CVCase.College_Career_Goal_Statement__c}" label="College And Career Goal Statement (Limitation: 1200 characters max)"  disabled = "true"/>
                                
                            </div>
                            <div class=" col-sm-6">
                                <lightning:textarea name="rolestitlesheld"  class="alignField modal__FontSize col_14" value="{!v.CVCase.Services_roles_titles__c}" label="School/Tribal/Church organizations or services, roles/titles held, etc.(Limitation: 1200 characters max)" disabled = "true"/>
                                
                            </div>
                        </div>
                        <div class="row ">
                            <div class=" col-sm-6">
                                <lightning:textarea name="List_of_current_medical_issues"  class="alignField modal__FontSize col_14" value="{!v.CVCase.List_of_current_medical_issues__c}" label="List of current medical/health issues we should be aware of (Limitation: 1200 characters max)" disabled = "true" />
                                
                            </div>
                            <div class=" col-sm-6">
                                <lightning:textarea name="List_of_current_medications"  class="alignField modal__FontSize col_14" value="{!v.CVCase.List_of_current_medications__c}" label="List of current medications and medication routine (Limitation: 1200 characters max)" disabled = "true" />
                                
                            </div>
                        </div>
                        
                        <div class="row ">
                            <!-- Income Files Section-->
                            <div class=" col-sm-6">
                                <p style="margin-top:10px !important; margin-bottom:-10px !important; font-weight: 500;">
                                    Letter of Recommendation from school teacher/counselor/principal:
                                </p>
                                <aura:iteration items="{!v.incomeUploadedFilesList}" var="file">
                                    <lightning:layoutItem class = "custom_pillLayout"  size="6" mediumDeviceSize="3" padding="around-small">
                                        <lightning:pill class="slds-media slds-media_small slds-media_center slds-media__body custom_pill removeX" 
                                                        name="{!file.Id}" label="{!file.Title}"
                                                        onclick="{!c.previewFile}"
                                                        >
                                            <aura:set attribute="media">
                                                <lightning:icon style="fill:#43b02a;" class="icn" iconName="utility:file"  alternativeText="Labrador Image"/>
                                            </aura:set>
                                        </lightning:pill>
                                    </lightning:layoutItem>
                                </aura:iteration>
                            </div>
                            
                            <!--/End Income Files Section-->
                            
                            <!-- Custody Files Section-->
                            <div class=" col-sm-6">
                                <p style="margin-top:10px !important; margin-bottom:-7px !important; font-weight: 500;">Current Photo: 
                                </p>
                                <aura:iteration items="{!v.custodyUploadedFilesList}" var="file">
                                    <lightning:layoutItem  class = "custom_pillLayout" size="6" mediumDeviceSize="3" padding="around-small">
                                        <lightning:pill class="slds-media slds-media_small slds-media_center slds-media__body custom_pill removeX" 
                                                        name="{!file.Id}" label="{!file.Title}"
                                                        onclick="{!c.previewFile}">
                                            <aura:set attribute="media">
                                                <lightning:icon style="fill:#43b02a;" class="icn" iconName="utility:file"  alternativeText="Labrador Image"/>
                                            </aura:set>
                                        </lightning:pill>
                                    </lightning:layoutItem>
                                </aura:iteration>                            
                            </div>
                            <!--/End Custody Files Section-->
                        </div>
                        
                        
                        
                        <div class=" text-center">
                            <lightning:button label="Edit" class="btn mBtn " variant="brand" onclick="{!c.hideModal}"/>     
                            <lightning:button label="Submit" disabled="{!v.isActive2}" class="btn mBtn " variant="brand" onclick="{!c.save}"/>
                        </div> 
                        <lightning:spinner aura:id="Spinner2" alternativeText="Processing.." title="Processing.." variant="brand" size="large" class="slds-hide"/>     
                    </form>
                    
                    
                </div> <!--/Modal Body-->
                
            </div>
        </section>
        
        <div class="slds-backdrop slds-backdrop_open"></div>
        
    </aura:if> 
    <!--/Review Section-->  
</aura:component>